<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.cmms.modules.xdgl.grdkgl.mapper.GrdkJtspMapper">


    <update id="updById">
        update CAMS_ZCSX_GRXDJTSP set jtspzzzt = 1 where id = #{id}
    </update>

    <select id="getDbxx" resultType="org.cmms.modules.xdgl.grdkgl.entity.GrdkDbxx" parameterType="java.lang.String">
        select * from CAMS_JBXX_GRXD_DBXX where zjhm = #{zjhm}
    </select>

</mapper>
